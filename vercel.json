{
  "devCommand": "yarn workspace @peoplenotplatforms/client start",
  "buildCommand": "yarn workspace @peoplenotplatforms/client build",
  "outputDirectory": "client/build",
  "rewrites": [
    {
      "source": "/login",
      "destination": "/api/login"
    },
    {
      "source": "/logout",
      "destination": "/api/logout"
    },
    {
      "source": "/graphql",
      "destination": "/api/graphql"
    }
  ]
}
